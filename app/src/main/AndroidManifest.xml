<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.workoutbuddy">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.WorkoutNav.EndWorkoutActivity" />
        <activity android:name=".activities.WorkoutNav.WorkoutInProgressActivity" />
        <activity android:name=".activities.WorkoutNav.StartWorkoutActivity" />
        <activity android:name=".activities.ExercisePopupWindow" />
        <activity android:name=".activities.NewWorkoutActivity" />
        <activity android:name=".activities.NewExerciseActivity" />
        <activity android:name=".activities.LoginSignup.SignupActivity" />
        <activity android:name=".activities.LoginSignup.LoginActivity">

        </activity>
        <activity
            android:name=".activities.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>